{
  "expo": {
    "name": "TimeCraft",
    "slug": "timecraft",
    "version": "1.0.0",
    "platforms": [
      "android"
    ],
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.timecraft.app",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#007AFF"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d30aa4ca-0dcd-41fe-981e-6c350efa2b08"
      }
    }
  }
}
